    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@cname">
            <Value>=Parameters!cname.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@city">
            <Value>=Parameters!city.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT          客戶編號, 公司名稱, 連絡人, 連絡人職稱, 電話, 地址
FROM              客戶
WHERE 公司名稱 LIKE ('%'+@cname+'%') AND SUBSTRING(地址,1,3) IN(@city)</CommandText>

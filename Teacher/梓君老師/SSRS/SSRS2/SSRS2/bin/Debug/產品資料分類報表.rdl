  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>北風連接_Windows驗證_</DataSourceName>
        <CommandText>SELECT a.產品編號,a.產品,c.供應商,B.類別名稱,A.單位數量,A.單價,A.庫存量,A.產品圖片
FROM 產品資料 AS A JOIN 產品類別 AS B ON A.類別編號=B.類別編號
	JOIN 供應商 AS C ON A.供應商編號=c.供應商編號</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>

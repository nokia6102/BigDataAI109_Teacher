  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DS1</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@year">
            <Value>=Parameters!year.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>SELECT YEAR(A.訂單日期) AS 訂單年,D.類別名稱
	,SUM(B.數量) AS 銷售數量
	,SUM(ROUND(B.單價*B.數量*(1-折扣),0)) AS 營收
FROM 訂貨主檔 AS A JOIN 訂貨明細 AS B ON A.訂單號碼=B.訂單號碼
	JOIN 產品資料 AS C ON B.產品編號=C.產品編號
	JOIN 產品類別 AS D ON C.類別編號=D.類別編號
WHERE YEAR(A.訂單日期)=@year
GROUP BY YEAR(A.訂單日期),D.類別名稱</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
      <Fields>
        <Field Name="訂單年">
          <DataField>訂單年</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
        <Field Name="類別名稱">
          <DataField>類別名稱</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="銷售數量">
